{
    "compilerOptions": {
      "module": "ESNext",  // Atau "ES6"
      "target": "ESNext",  // Atau "ES6"
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
    },
    "include": ["*.ts"] // Menyesuaikan dengan lokasi file TypeScript Anda
  }
  